{
  "name": "@js-omar/create-ts-project",
  "displayName": "Create typescript project",
  "version": "1.1.0",
  "description": "Create new typescript project",
  "main": "./bin/index.js",
  "bin": {
    "js-omar-create-ts-project": "bin/index.js"
  },
  "scripts": {
    "prepare": "husky install",
    "fix:format": "prettier --write .",
    "fix:lint": "eslint . --ext ts --ext js --fix",
    "check:cspell": "cspell",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext ts --ext js",
    "test:all": "npm run check:cspell && npm run check:format && npm run check:lint"
  },
  "homepage": "https://github.com/js-omar/create-ts-project#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-omar/create-ts-project.git"
  },
  "keywords": [
    "javascript"
  ],
  "author": {
    "name": "Omar Elsayed",
    "email": "challengeromar97@gmail.com",
    "url": "https://ng-omar.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/js-omar/create-ts-project/issues"
  },
  "devDependencies": {
    "@js-omar/eslint-config": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cspell": "^5.19.7",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10"
  }
}
